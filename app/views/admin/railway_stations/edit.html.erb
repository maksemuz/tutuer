<h1><%= t('common.edit') %></h1>

<%= form_for [:admin, @railway_station] do |f| %>

    <%= render 'common/errors', resource: @railway_station %>

    <div class="field">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>

    <div class="actions">
      <%= f.submit "#{@railway_station.new_record? ? t('common.create') : t('common.update')}" %>
    </div>
<% end %>

<%= link_to t('common.show'), admin_railway_station_path %> |
<%= link_to t('common.back'), admin_railway_stations_path %>
